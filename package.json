    "type": "module",
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "bcrypt": "^5.0.1",
        "dotenv": "^16.0.3",
        "fs": "0.0.1-security",
        "mariadb": "^3.1.0",
        "simple-json-db": "^2.0.0",
        "uuid": "^8.3.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/patrick11514/ImageGallery.git"
    }
}
