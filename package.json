{
    "name": "screenshot_browser",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn ./src/index.ts",
        "folders": "mkdir -p storage cache logs",
        "build": "tsc -p tsconfig.json && pnpm folders",
        "start": "node ./build/index.js",
        "clear": "rm -r build/*",
        "clearCache": "rm -r cache/*",
        "buildCss": "tailwindcss -i public/css/style.tw.css -o public/css/style.css",
        "devCss": "tailwindcss -i public/css/style.tw.css -o public/css/style.css --watch",
        "buildJs": "tsc -p public/tsconfig.json",
        "devJs": "tsc -p public/tsconfig.json --watch"
    },
    "keywords": [],
    "author": "Patrik MintÄ›l",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.1",
        "cli-color": "^2.0.3",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "fs": "0.0.1-security",
        "sharp": "^0.31.2",
        "simple-json-db": "^2.0.0",
        "strip-color": "^0.1.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.2",
        "@types/cli-color": "^2.0.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.14",
        "@types/node": "^18.11.15",
